#HOW TO CREATE A CALCULATOR

first=input("enter first number:")
operator=input("enter operator (+ , - , * , / , %):")
second=input("enter second number:")

first= int(first)
second= int(second)

if operator=="+":
    print(first + second)
elif operator=="-":
    print(first - second)
elif operator=="*":
    print(first * second)
elif operator=="/":
    print(first / second)
elif operator=="%":
    print(first % second)
else:
    print("invalid operation")
#HOW TO CREATE A CALCULATOR

first=input("enter first number:")
operator=input("enter operator (+ , - , * , / , %):")
second=input("enter second number:")

first= int(first)
second= int(second)

if operator=="+":
    print(first + second)
elif operator=="-":
    print(first - second)
elif operator=="*":
    print(first * second)
elif operator=="/":
    print(first / second)
elif operator=="%":
    print(first % second)
else:
    print("invalid operation")
        #HOW TO CREATE A CALCULATOR

first=input("enter first number:")
operator=input("enter operator (+ , - , / , % ,*):")
second=input("enter second number:")

first=int(first)
second=int(second)

if operator=="+":
    print(first + second)
elif operator=="-":
    print(first - second)
elif operator=="*":
    print(first * second)
elif operator=="/":
    print(first / second)
elif operator=="%":
    print(first % second)
else:
    print("invalid operation")

    
    
   



